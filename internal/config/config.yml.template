config:
    server:
        host: ${APP_SERVER_HOST}
        port: ${APP_SERVER_PORT}
    client:
        url: ${APP_CLIENT_URL}
        username: ${APP_CLIENT_USERNAME}
        password: ${APP_CLIENT_PASSWORD}
        timeout: ${APP_CLIENT_TIMEOUT}
    database:
        username: ${APP_DATABASE_USERNAME}
        password: ${APP_DATABASE_PASSWORD}
        host: ${APP_DATABASE_HOST}
        port: ${APP_DATABASE_PORT}
        dbname: ${APP_DATABASE_DBNAME}
